<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="communityController"
			class="com.ktds.community.web.CommunityController">
		<property name="communityService"  ref="communityService"></property>
	</bean>
	
	
	<bean id="memberController"
			class="com.ktds.community.member.web.MemberController">
		<property name="memberService" ref="memberService"></property>
		<property name="communityService" ref="communityService"></property>
	</bean>
	
	<bean id="myPageController"	class="com.ktds.community.member.web.MyPageController">
		<property name="memberService" ref="memberService"></property>
		<property name="communityService" ref="communityService"></property>
	</bean>
		
</beans>
